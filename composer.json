{
    "name": "aboutcoders/scheduler-bundle",
    "type": "symfony-bundle",
    "description": "A symfony bundle to process schedules base on CRON expressions",
    "keywords": ["cron", "schedule", "command"],
    "homepage": "http://aboutcoders.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hannes.schulz@aboutcoders.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.3",
        "abc/scheduler": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/browser-kit": "^4.3",
        "symfony/console": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "autoload": {
        "psr-4": { "Abc\\SchedulerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Abc\\SchedulerBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
